{
    "name": "Rest reminder for eye straining",
    "version": "1.0.3",
    "description": "Rest reminder for eye straining",
    "icons": {"16": "icon16.png"},
    "permissions": ["tabs", "notifications"],
    "options_page": "options.html",
    "background": { "scripts": ["background.js"] },
    "manifest_version": 2,
    "web_accessible_resources": ["48.png", "blinking-eyes.gif"],
    
    "browser_action": {
        "default_popup": "options.html",
        "default_icon": "icon16.png"
    }
}
